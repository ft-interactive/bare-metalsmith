{{> head}}
  <body>
    {{> tracking-snippet}}

    {{> header}}

    <main role="main">
      <article class="article">
        <div class="article-head o-grid-container">
          <div class="o-grid-row">
            <header data-o-grid-colspan="12 S12 Scenter M10 L8">
              {{> share}}

              {{> article-header}}
            </header>
          </div>

          <div class="article-body o-typography-body-wrapper">
            {{{contents}}}
            <div class="aggregate o-grid-row">
              <div data-o-grid-colspan="12"><h2>Change in total value since JuneÂ 24</h2>
                <div class="o-grid-row">
                  {{#each agg}}
                    {{> agg-item}}
                  {{/each}}
                </div>
              </div>
            </div>

            <!-- Parse Bertha data as feed object, iterating over the array -->
            {{#each (sortBy feed 'timestamp')}}
              {{> feed-item}}
            {{/each}}
          </div>
         </div>
      </article>
    </main>

    {{> footer}}
    <script src="scripts/main.js"></script>
  </body>
</html>
